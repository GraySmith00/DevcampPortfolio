<h1>Angular Portfolio items</h1>

<% @angular_portfolio_items.each do |portfolio_item| %>
  <h2><%= link_to portfolio_item.title, portfolio_show_path(portfolio_item.id) %></h2>
  <p><%= portfolio_item.subtitle %></p>
  <p><%= portfolio_item.body %></p>
  <p><%= image_tag portfolio_item.thumb_image unless portfolio_item.thumb_image.nil? %></p>
  <hr/>
<% end %>

<%= link_to 'Back', portfolios_path %>